pipeline{
    agent any 
    tools{
        maven "maven3.9.5"
    }

    stages{
        stage('clonecode4scm'){
            steps{
            sh "echo 'this is declarative pipeline'"
            git branch: 'nov17', url: 'https://github.com/Lion-Technology-Solutions/liontech-online-library-web-app.git' 
            }
        }

        stage('build+test'){
            step{
                sh "mvn clean package"
            }
        }





    }









}